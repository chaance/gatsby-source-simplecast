{
  "name": "gatsby-source-simplecast",
  "version": "2.1.0",
  "description": "Gatsby plugin to load podcast episodes from Simplecast.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "dev": "cross-env NODE_ENV=development yarn watch",
    "prepare": "cross-env NODE_ENV=production yarn build",
    "test": "echo \"Error: no test specified\""
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "simplecast"
  ],
  "author": "Chance Strickland <hi@chancedigital.io>",
  "homepage": "https://github.com/chancestrickland/gatsby-source-simplecast#readme",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/chancestrickland/gatsby-source-simplecast"
  },
  "bugs": {
    "url": "https://github.com/chancestrickland/gatsby-source-simplecast/issues"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "gatsby-node-helpers": "^0.3.0",
    "map-obj": "^4.1.0",
    "node-fetch": "^2.6.0",
    "quick-lru": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-preset-gatsby-package": "^0.2.14",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "prettier": "^1.19.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
